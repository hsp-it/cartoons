        <p>BAGGAGE SWINDLER</p>
        <p>BAGGA</p>
        <p>No</p>
      </div>
      <div type="image_transcription" n="10" facs="#Pit_5">
        <head><supplied resp="editorial">Pit 5</supplied></head>
        <p>MONEY CHANGER</p>
      </div>
